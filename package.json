{
  "name": "openapi-mock-server",
  "version": "0.0.1",
  "description": "Generate mock API based on static content",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "rimraf dist/ && babel ./ --out-dir dist/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files",
    "start": "npm run build && node ./dist/index.js"
  },
  "keywords": [
    "open-api",
    "mock",
    "testing",
    "front-end first"
  ],
  "author": "Ahmed Badawi (ahmedabadawi@gmail.com)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "commander": "^2.19.0"
  }
}
